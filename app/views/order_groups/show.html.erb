<div>
  <h1>Orders</h1>
</div>
<table class="table">
  <thead>
    <tr>
      <th scope="col">combo_name</th>
      <th scope="col">user</th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order|%>
      <tr>
        <td><%= order.combo.name %></td>
        <td><%= order.user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
